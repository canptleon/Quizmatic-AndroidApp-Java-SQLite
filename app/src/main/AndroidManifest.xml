<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizmatic">

    <application
        android:allowBackup="true"
        android:icon="@drawable/quiz_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/quiz_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.LicLevelActivity"></activity>
        <activity android:name=".activities.CultureLevelActivity" />
        <activity android:name=".activities.SportLevelActivity" />
        <activity android:name=".activities.MathLevelActivity" />
        <activity android:name=".activities.ScienceLevelActivity" />
        <activity android:name=".activities.CategoryActivity" />
        <activity android:name=".activities.QuizActivity" />
        <activity android:name=".activities.PlayActivity" />
        <activity android:name=".activities.ResultActivity" />
        <activity android:name=".activities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>